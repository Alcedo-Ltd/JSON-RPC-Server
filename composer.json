{
  "name": "alcedo/json-rpc-server",
  "description": "JSON-RPC server for executing functions remotely.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kiril Savchev",
      "email": "k.savchev@gmail.com",
      "homepage": "https://alcedo.bg"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "Alcedo\\JsonRpc\\Server\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alcedo\\Tests\\JsonRpc\\Server\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "squizlabs/php_codesniffer": "^4.0",
    "phpmd/phpmd": "^2.15"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
